  <Gallery />
  <Normativas />
  <Beneficios />
  <Clientes />
  <Faq />
  <Contacto />

  <BlurGradientExample />
  <BlurGradientAdvanced />
  <BlurGradientHero />

  <Footer />
</Layout>
